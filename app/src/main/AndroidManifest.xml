<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.camant.moneycrab">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TransactionPlusActivity" />
        <activity android:name=".activity.TransactionMinusActivity" />
        <activity android:name=".activity.TransactionUpdateActivity" />
        <activity android:name=".activity.CurrencyActivity" />
        <activity android:name=".activity.CategoryActivity" />
        <activity android:name=".activity.AccountActivity"></activity>
    </application>

</manifest>